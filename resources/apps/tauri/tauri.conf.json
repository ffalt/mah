{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mah",
  "version": "1.14.0",
  "identifier": "io.github.ffalt.mah",
  "build": {
    "removeUnusedCommands": true,
    "beforeDevCommand": "cd ../../ && npm run start:development:apps",
    "devUrl": "http://0.0.0.0:4200",
    "beforeBuildCommand": "cd ../../ && npm run build:apps",
    "frontendDist": "../../../dist/"
  },
  "app": {
    "windows": [
      {
        "title": "Mah",
        "width": 1024,
        "height": 800,
        "maximized": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": "-"
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}

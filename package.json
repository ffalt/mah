{
	"name": "mah",
	"version": "1.4.0",
	"author": "ffalt",
	"license": "MIT",
	"description": "a html5 mahjong solitaire game",
	"repository": "https://github.com/ffalt/mah.git",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"build:prod": "npm run build:deploy-release",
		"build:analyze:prod": "ng build --configuration production --aot --source-map",
		"build:prebuild": "npm run clean",
		"build:deploy-release": "npm run build:prebuild && ng build --configuration production --aot --base-href \".\"",
		"build:deploy-root": "npm run build:prebuild && ng build --configuration production --aot --base-href \"/\"",
		"build:deploy-ghpages": "npm run build:prebuild && ng build --configuration production --aot --base-href \"https://ffalt.github.io/mah/\"",
		"build:releaselog": "conventional-changelog -p angular -r 2 -o RELEASELOG.md",
		"explore-map-explorer": "source-map-explorer dist/**/*.js",
		"explore": "npm run build:analyze:prod && npm run explore-map-explorer",
		"release": "standard-version -a",
		"update:angular": "ng update @angular/cli @angular/core",
		"clean": "rimraf dist",
		"test": "jest",
		"coverage": "jest --coverage true",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "13.3.9",
		"@angular/common": "13.3.9",
		"@angular/compiler": "13.3.9",
		"@angular/core": "13.3.9",
		"@angular/platform-browser": "13.3.9",
		"@angular/platform-browser-dynamic": "13.3.9",
		"@angular/platform-server": "13.3.9",
		"@ngx-translate/core": "14.0.0",
		"rxjs": "7.5.5",
		"tslib": "2.4.0",
		"tsutils": "3.21.0",
		"zone.js": "~0.11.5"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "13.1.0",
		"@angular-devkit/build-angular": "13.3.6",
		"@angular-eslint/builder": "13.2.1",
		"@angular-eslint/eslint-plugin": "13.2.1",
		"@angular-eslint/eslint-plugin-template": "13.2.1",
		"@angular-eslint/schematics": "13.2.1",
		"@angular-eslint/template-parser": "13.2.1",
		"@angular/cli": "13.3.6",
		"@angular/compiler-cli": "13.3.9",
		"@angular/language-service": "13.3.9",
		"@types/jest": "27.5.1",
		"@types/node": "17.0.35",
		"@types/webpack": "5.28.0",
		"@typescript-eslint/eslint-plugin": "5.25.0",
		"@typescript-eslint/parser": "5.25.0",
		"conventional-changelog-cli": "2.2.2",
		"eslint": "8.16.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsdoc": "39.3.0",
		"eslint-plugin-no-null": "latest",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-rxjs": "5.0.2",
		"eslint-plugin-unicorn": "42.0.0",
		"is-promise": "4.0.0",
		"jest": "28.1.0",
		"jest-preset-angular": "12.0.1",
		"ng-mocks": "13.5.2",
		"rimraf": "3.0.2",
		"source-map-explorer": "^2.5.2",
		"standard-version": "9.5.0",
		"typescript": "4.6.4",
		"webpack": "5.72.1"
	},
	"contributors": [
		{
			"name": "Heimen Stoffels",
			"url": "https://github.com/Vistaus"
		}
	]
}

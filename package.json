{
  "name": "mah",
  "version": "1.1.9",
  "author": "ffalt",
  "license": "MIT",
  "description": "a html5 mahjong solitaire game",
  "repository": "https://github.com/ffalt/mah.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build:deploy-release",
    "build:prebuild": "node ./build-pre.js",
    "build:deploy-release": "npm run build:prebuild && ng build --prod --aot --base-href \".\"",
    "build:deploy-root": "npm run build:prebuild && ng build --prod --aot --base-href \"/\"",
    "build:deploy-ghpages": "npm run build:prebuild && ng build --prod --aot --base-href \"https://ffalt.github.io/mah/\"",
    "deploy": "deplokay -c deplokay-ghpages.json",
    "release": "deplokay -c deplokay-release.json",
    "test": "jest",
    "coverage": "jest --coverage true",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/forms": "8.2.13",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/platform-server": "8.2.13",
    "@ngx-translate/core": "11.0.1",
    "classlist.js": "1.1.20150312",
    "core-js": "3.3.6",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "tsutils": "3.17.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.0.0",
    "@angular-devkit/build-angular": "0.803.17",
    "@angular/cli": "8.3.17",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "codelyzer": "5.2.0",
    "deplokay": "0.0.11",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "ng-mocks": "8.1.0",
    "rxjs-tslint": "0.1.7",
    "rxjs-tslint-rules": "4.26.1",
    "tslint": "5.20.0",
    "tslint-etc": "1.9.2",
    "typescript": "3.5.3"
  }
}
